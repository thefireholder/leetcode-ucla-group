class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        unordered_map<int, int> m;
        
        for (int i = 0; i < nums.size(); i++) {
            
            if (m.find(nums[i]) != m.end())
                return true;
            
            m[nums[i]] = i;
        }
        
        return false;
    }
};


Runtime: 32 ms, faster than 70.16% of C++ online submissions for Contains Duplicate.
Memory Usage: 20.3 MB, less than 36.12% of C++ online submissions for Contains Duplicate.
